{
  "name": "fitprohub",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "homepage": "https://prakash-solace.github.io/wireframe-fitprohub",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "cra-template-pwa": "2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.9.3",
    "react-slick": "^0.30.3",
    "recharts": "^3.1.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dist": "npm run build && electron-builder",
    "deploy": "gh-pages -d build"
  },
  "build": {
    "appId": "com.yourcompany.fitprohub",
    "productName": "FitProHub",
    "files": [
      "build/**/*",
      "electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
